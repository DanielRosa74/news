{
  "directoryListing": false,
  "rewrites": [
    {
      "source": "**/rss",
      "destination": "/rss.xml"
    }
  ],
  "redirects": [
    {
      "source": "/@:username",
      "destination": "/italian/news/author/:username",
      "type": 302
    },
    {
      "source": "/tagged/:tagName",
      "destination": "/italian/news/tag/:tagName",
      "type": 302
    },
    {
      "source": "/archive/**",
      "destination": "/italian/news/",
      "type": 302
    },
    {
      "source": "/our-sponsors",
      "destination": "/italian/news/sponsors",
      "type": 302
    },
    {
      "source": "/about-freecodecamp",
      "destination": "/italian/news/about",
      "type": 302
    }
  ]
}